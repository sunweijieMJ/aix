{
  "name": "aix",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "turbo dev --concurrency=20",
    "build": "turbo build",
    "build:filter": "turbo build --filter",
    "clean": "turbo clean",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:unit": "vitest run --project unit",
    "test:stories": "vitest run --project storybook",
    "test:ui": "npx vitest --ui",
    "type-check": "tsc --noEmit",
    "commit": "cross-env NODE_OPTIONS=\"--experimental-transform-types --disable-warning=ExperimentalWarning\" czg",
    "cspell": "cspell",
    "format": "prettier . --write",
    "lint:md": "markdownlint-cli2 '**/*.{md}'",
    "gen": "tsx scripts/gen/index.ts",
    "link:setup": "tsx scripts/link/setup.ts",
    "link:publish": "tsx scripts/link/publish.ts",
    "link:push": "tsx scripts/link/push.ts",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build --output-dir=dist/storybook",
    "storybook:preview": "npx serve dist/storybook -p 6006",
    "gen:docs": "tsx scripts/docs/gen-docs.ts",
    "sync:docs": "tsx scripts/docs/sync-docs.ts",
    "docs:gen": "pnpm gen:docs && pnpm sync:docs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "pnpm docs:gen && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:docs-all": "pnpm storybook:build && pnpm docs:build",
    "pre": "tsx scripts/publish/index.ts",
    "postinstall": "npx husky"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,less,vue}": [
      "stylelint --fix"
    ],
    "*.md": [
      "npm run lint:md"
    ],
    "*": [
      "pretty-quick --staged"
    ]
  },
  "peerDependencies": {
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@commitlint/format": "^20.4.0",
    "@commitlint/types": "^20.4.0",
    "@kit/eslint-config": "workspace:*",
    "@kit/stylelint-config": "workspace:*",
    "@kit/typescript-config": "workspace:*",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/addon-links": "^10.2.10",
    "@storybook/addon-vitest": "^10.2.10",
    "@storybook/vue3": "^10.2.10",
    "@storybook/vue3-vite": "^10.2.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^25.3.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.24",
    "chalk": "^5.6.2",
    "commitizen": "^4.3.1",
    "conventional-changelog-atom": "^5.0.0",
    "cross-env": "^10.1.0",
    "cspell": "^9.6.4",
    "cz-git": "^1.12.0",
    "czg": "^1.12.0",
    "eslint": "^10.0.0",
    "eta": "^4.5.1",
    "glob": "^13.0.2",
    "husky": "^9.1.7",
    "inquirer": "^13.2.2",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.21.0",
    "mermaid": "^11.12.3",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.1.3",
    "rollup": "^4.59.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.97.3",
    "storybook": "^10.2.10",
    "stylelint": "^17.1.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "turbo": "^2.8.10",
    "typescript": "5.9.3",
    "unplugin-vue": "^7.1.1",
    "vite": "^7.3.1",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitest": "^4.0.18",
    "vue": "^3.5.28",
    "vue-docgen-api": "^4.79.2",
    "vue-tsc": "^3.2.5"
  }
}
